algoritmo "Dólar pra real"

var

//Declaração de Variáveis
dolares: real
valor_reais: real

inicio

//Entrada de dados
escreva("Informe o seu salário em dólares: $ ")
leia(dolares)

//Cálculo para converter dólar pra real na cotação atual
valor_reais<- 5.17*dolares

//Saída de dados
escreval("")
escreval("Seu salário convertido pra reais é R$: ", valor_reais)

fimalgoritmo
