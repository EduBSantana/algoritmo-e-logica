algoritmo "Operadores relacionais"

var

//Declaração de Variáveis
numero1, numero2, numero3: inteiro

inicio

//Seção de Comandos
numero1 <- 7
numero2 <- 3
numero3 <- 10

//Retorna valor lógico(boolean) de acordo com resultado da operação lógica
escreval(numero3 >= numero1 + numero2)

fimalgoritmo
