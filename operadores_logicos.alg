algoritmo "Operadores lógicos"

var

   //Declaração de Variáveis
   numero1, numero2, numero3: inteiro

inicio

   //Seção de Comandos
   
   //Atribuindo valores às variáveis
   numero1 <- 6
   numero2 <- 9
   numero3 <- 15
   
   //Nega o resultado lógico(boolean) que a comparação lógica resultar
   escreval( nao( (numero1 >= numero2) e (numero3 = (numero1 + numero2)) ) )

fimalgoritmo
